#!/usr/bin/env bash

# Easier navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Shortcuts for commonly used directories
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"

# Listing
alias l="ls -lAF -G"

# Show local ip
alias localip="hostname -I"